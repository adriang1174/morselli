<!-- BEGIN Record hipotecas -->
<form name="{HTMLFormName}" action="{Action}" method="post">
  <table cellspacing="0" cellpadding="0" width="600" border="0">
    <tr>
      <td valign="top">
        <table class="Header" cellspacing="0" cellpadding="0" border="0">
          <tr>
            <td class="HeaderLeft"><img src="../Styles/SandBeach/Images/Spacer.gif" border="0"></td> 
            <th>Datos de la&nbsp;Hipoteca </th>
 
            <td class="HeaderRight"><img src="../Styles/SandBeach/Images/Spacer.gif" border="0"></td>
          </tr>
        </table>
 
        <table class="Record" cellspacing="0" cellpadding="0">
          <!-- BEGIN Error -->
          <tr class="Error">
            <td colspan="4">{Error}</td>
          </tr>
          <!-- END Error -->
          <tr class="Controls">
            <th colspan="4">Deudor/es: {data_deudor}</th>
          </tr>
 
          <tr class="Controls">
            <th colspan="4">Propiedad: {data_prop}</th>
          </tr>
 
          <tr class="Controls">
            <th>Nro. Hipoteca</th>
 
            <td><input id="hipotecasidhipoteca" value="{idhipoteca}" name="{idhipoteca_Name}"></td> 
            <td>&nbsp;</td> 
            <td>&nbsp;</td>
          </tr>
 
          <tr class="Controls">
            <th>Monto hipoteca</th>
 
            <td><input id="hipotecasmontohipoteca" maxlength="20" value="{montohipoteca}" name="{montohipoteca_Name}" size="20"></td> 
            <td>&nbsp;Moneda</td> 
            <td>
              <select id="hipotecasListBox1" name="{ListBox1_Name}">
                <option value="" selected>Seleccionar Valor</option>
                {ListBox1_Options}
              </select>
 </td>
          </tr>
 
          <tr class="Controls">
            <th>Fecha inicio</th>
 
            <td><input id="hipotecasfechainicio" maxlength="100" size="10" value="{fechainicio}" name="{fechainicio_Name}">
              <!-- BEGIN DatePicker DatePicker_fechainicio --><a href="javascript:showDatePicker('{Name}','{FormName}','{DateControl}');" id="hipotecasDatePicker_fechainicio"><img alt="Show Date Picker" src="../Styles/SandBeach/Images/DatePicker.gif" border="0"></a><!-- END DatePicker DatePicker_fechainicio --></td> 
            <td>Fecha fin&nbsp;</td> 
            <td><input id="hipotecasfechafin" maxlength="100" size="10" value="{fechafin}" name="{fechafin_Name}">
              <!-- BEGIN DatePicker DatePicker_fechafin --><a href="javascript:showDatePicker('{Name}','{FormName}','{DateControl}');" id="hipotecasDatePicker_fechafin"><img alt="Show Date Picker" src="../Styles/SandBeach/Images/DatePicker.gif" border="0"></a>&nbsp;<!-- END DatePicker DatePicker_fechafin --></td>
          </tr>
 
          <tr class="Controls">
            <th>Honorarios (%)</th>
 
            <td><input id="hipotecashonorario" value="{honorario}" name="{honorario_Name}"></td> 
            <td>Porc. Liq (%)</td> 
            <td><input id="hipotecasliqhip" value="{liqhip}" name="{liqhip_Name}"></td>
          </tr>
 
          <tr class="Controls">
            <th colspan="4">&nbsp; <input id="hipotecasidestado" type="hidden" value="{idestado}" name="{idestado_Name}"><input id="hipotecasidpropiedad" type="hidden" value="{idpropiedad}" name="{idpropiedad_Name}"></th>
          </tr>
 
          <tr class="Bottom">
            <td align="right" colspan="4">
              <!-- BEGIN Button Button_Insert --><input class="Button" id="hipotecasButton_Insert" type="submit" value="Agregar" name="{Button_Name}"><!-- END Button Button_Insert -->
              <!-- BEGIN Button Button_Update --><input class="Button" id="hipotecasButton_Update" type="submit" value="Enviar" name="{Button_Name}"><!-- END Button Button_Update -->
              <!-- BEGIN Button Button_Delete --><input class="Button" id="hipotecasButton_Delete" type="submit" value="Borrar" name="{Button_Name}"><!-- END Button Button_Delete --></td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
</form>
<!-- END Record hipotecas -->&nbsp;&nbsp; 
