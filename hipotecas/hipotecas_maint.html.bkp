<script src="../ajax.js" type="text/javascript"></script>
<script language="JavaScript" type="text/javascript">
//Begin ajax material lookup
                var ajax = new Array();
                var indexThis = 0;
                 ajax[indexThis] = new sack();
        var currentCodBarras=false;
                
        function getFichaData(nameVar,idVar)
        {
                        var cod_barras = document.getElementById(idVar).value;
                        var str = nameVar;
                        var it = str.substr(str.indexOf("_")+1,str.length-str.indexOf("_"));
                        //alert(nameVar);
                                                var str2 = nameVar;
                                                var str3 = str2.substr(0,str2.indexOf("_"));

                                                                ajax[indexThis].requestFile = 'getFicha.php?'+str3+'='+cod_barras+'&i='+it;   // Specifying which file to get
                        ajax[indexThis].onCompletion = function(){ showFichaData(it); } ;
                         ajax[indexThis].runAJAX();         // Execute AJAX function                        
                 
        }
        
        function showFichaData(it)
        {
                var formObj = document.forms['fichashipotecas'];     
                                eval(ajax[indexThis].response);
                                if (formObj.errorAjax.value=="true")
                                {
                                        alert('Ficha no encontrada');
                                        eval("formObj.idficha_"+it+".focus()")
                                }
                                else
                                {
                                        eval("formObj.monto_"+it+".focus()")
                                }
        }
 </script>
